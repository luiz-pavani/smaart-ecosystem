{
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": true
  },
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/lp", "destination": "/landing/index.html" },
    { "source": "/lp/(.*)", "destination": "/landing/$1" },
    { "source": "/", "destination": "/index.html" }
  ]
}
